{
  "name": "happo.io",
  "version": "1.1.0",
  "description": "Visual diffing for UI components",
  "main": "./build/index.js",
  "bin": {
    "happo": "./build/cli.js",
    "happo-ci-travis": "./bin/happo-ci-travis",
    "happo-ci-circleci": "./bin/happo-ci-circleci",
    "happo-ci": "./bin/happo-ci"
  },
  "scripts": {
    "test": "jest",
    "build": "babel src --ignore 'test/.*' --out-dir build --copy-files",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enduire/happo.io.git"
  },
  "keywords": [
    "visual",
    "diffing",
    "ui",
    "testing",
    "snapshots"
  ],
  "author": "Henric Trotzig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enduire/happo.io/issues"
  },
  "homepage": "https://github.com/enduire/happo.io#readme",
  "jest": {
    "testMatch": [
      "**/*-test.js*"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "build/.*"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "commander": "^2.11.0",
    "glob": "^7.1.2",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "jsonwebtoken": "^8.0.1",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.4",
    "require-relative": "^0.8.7",
    "source-map-support": "^0.5.4",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.6.0",
    "jest": "^20.0.4"
  },
  "peerDependencies": {}
}
